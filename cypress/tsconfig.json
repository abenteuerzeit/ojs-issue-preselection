        "target": "ES2020",
        "lib": ["ES2020", "DOM"],
        "types": ["cypress"],
        "allowJs": true,
        "checkJs": false,
        "noEmit": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": ["**/*.js", "**/*.d.ts"]
}
